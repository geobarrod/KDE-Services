[Desktop Entry]
Actions=addTimestampFileModified;
Icon=ks-clock
MimeType=all/all;
Type=Service
X-KDE-AuthorizeAction=shell_access
X-KDE-ServiceTypes=KonqPopupMenu/Plugin
X-KDE-Submenu=KDE-Services

[Desktop Action addTimestampFileModified]
Comment=Adds the timestamp of last modified date of file
Exec=mv "%f" "$(pwd)/$(date -r %f '+%%F')-$(basename %f)"
Icon=utilities-terminal
Name=Add Timestamp Prefix YYYY-MM-DD-... of file last modified to [File|Dir]name
TryExec=konsole
